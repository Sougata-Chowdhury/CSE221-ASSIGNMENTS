      "source": [
        "with open (\"input1a.txt\",'r') as file:\n",
        "  skip=next(file)\n",
        "  with open (\"Output1a.txt\",\"w\") as file2:\n",
        "    for i in file:\n",
        "      l=i.strip()\n",
        "      if int(l)%2==0:\n",
        "        file2.write(f\"{l} is an Even number.\\n\")\n",
        "      else:\n",
        "        file2.write(f\"{l} is an Odd number.\\n\")"
      ]
    }
  ]
}